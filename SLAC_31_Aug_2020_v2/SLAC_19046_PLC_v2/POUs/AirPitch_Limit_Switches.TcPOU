	axis:=axisAirPitch , 
	fTargetVelocity:=0.12 , 
	fStartPosition:= -3.7 , 
	fEndPosition:= 3, 
	nDivisions:= 20 , 
	fbAxisPower:= fbAxisPowerAirPitch );

