    fbAxisPowerAirPitch: MC_Power;
       
	bEnable: BOOL := TRUE;
    fOverride: LREAL := 100;
    		
	biLimFwd_AirPitch AT %I*: BOOL;
	biLimBwd_AirPitch AT %I*: BOOL;

	fbtest:FB_Test;
	I_moter_AirPich AT %I*: UINT;

END_VAR
]]></Declaration>
