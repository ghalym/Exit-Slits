    Axis:= axisAirPitch,
    Enable:= bEnable,
    Enable_Positive:= biLimFwd_AirPitch, // TRUE, //
    Enable_Negative:= biLimBwd_AirPitch, // TRUE, //
    Override:= fOverride,
    BufferMode:= ,
    Options:= ,
    Status=> ,
    Busy=> ,
    Active=> ,
    Error=> ,
    ErrorID=> );]]></ST>
    </Implementation>
